<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<ScrollPane fitToHeight="true" fitToWidth="true" minWidth="300.0" prefWidth="300.0" vbarPolicy="NEVER" HBox.hgrow="NEVER"
            xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ch.ethz.gametheory.gamecreator.controllers.SidePaneController">
    <VBox spacing="10.0">
        <padding>
            <Insets left="5.0" right="5.0" top="10.0"/>
        </padding>
        <TitledPane fx:id="TPPlayers" contentDisplay="RIGHT">
            <AnchorPane minHeight="-Infinity">
                <ListView fx:id="ListViewPlayers" focusTraversable="false" prefHeight="0.0"
                          AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                          AnchorPane.topAnchor="0.0"/>
            </AnchorPane>
        </TitledPane>
        <TitledPane fx:id="TPInformationSets" contentDisplay="RIGHT">
            <AnchorPane>
                <ListView fx:id="ListViewInformationSets" focusTraversable="false" prefHeight="0.0"
                          AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                          AnchorPane.topAnchor="0.0"/>
            </AnchorPane>
        </TitledPane>
        <TitledPane fx:id="TPNodes" contentDisplay="RIGHT">
            <graphic>
                <AnchorPane/>
            </graphic>
            <AnchorPane fx:id="AnchorPaneNodes"/>
        </TitledPane>
        <TitledPane fx:id="TPOutcomes" contentDisplay="RIGHT">
            <graphic>
                <AnchorPane/>
            </graphic>
            <AnchorPane fx:id="AnchorPaneOutcomes"/>
        </TitledPane>
    </VBox>
</ScrollPane>
